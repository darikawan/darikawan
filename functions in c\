#include <stdio.h>
#include <stdlib.h>
int main()
{
    float num,root;
    printf("enter your number:");
    scanf("%f",&num);
    root=abs(num);
    printf("the square root of %.2f = %.2f",num,root);
    return 0;
}

#include <stdio.h>
int addNumbers(int a, int b)
    {
    int result;
    result= a+b;
    return result;
    }
int main()
{
    int n1,n2,sum;
    printf("enter two numbers:");
    scanf("%d %d",&n1,&n2);
    sum=addNumbers(n1,n2);
    printf("the sum is %d",sum);
    return 0;
}
