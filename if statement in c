#include <stdio.h>
int main()
{
    int a,b;
    printf("enter two numebers:");
    scanf("%d %d,",&a,&b);
    if (a>b)
    {
        printf("%d is bigger than %d", a,b);
    }
    else if (a<b)
    {
        printf("%d is less than %d", a,b);
    }
    else
    {
        printf("the numbers are equal");
    }
    return 0;
}
